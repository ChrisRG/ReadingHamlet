        <!-- Substitutions -->
        <substitution method="pen" type="deletion" copyText="Antiquity forgot, custom not known, | 
            The ratifiers and props of every word, | 
            They cry chuse we Laertes for our King, | 
            Caps, hands, and tongues applaud it to the clouds, Laertes shall be King.| 
            ‘How chearfully on the false tail they cry; | 
            ‘O this is counter you false Danish doggs" amendedText=""/>       
        <substitution method="pen" type="deletion" copyText="With others" amendedText=""/>
        <substitution method="pen" type="deletion" copyText="The doors are broke" amendedText=""/>
        <substitution method="pen" type="deletion" copyText="Cries Cuckold to my father, brands and the Harlot | 
            Even here between the chaste brows |
            Of my true mother" amendedText=""/>
        <substitution method="pen" type="deletion" copyText="Acts little of his will: tell me Laertes Why thou art thus incenst" amendedText=""/>
        
